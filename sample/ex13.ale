$ printing a list in reverse order;
$  save the list as affixes

'a'read int+@+>a-b:
   get int+STDIN+b,read int+b+@;
   print int+@.
'a'print int+@+>a-b:
   a->b,(shift affix block+@,put int+STDOUT+b,:print int;+).


'root'put string+STDOUT+"Print numbers in reverse order. Enter the numbers closed by an X",
      put char+STDOUT+newline,
      read int+0,put char+STDOUT+newline.

'end'

